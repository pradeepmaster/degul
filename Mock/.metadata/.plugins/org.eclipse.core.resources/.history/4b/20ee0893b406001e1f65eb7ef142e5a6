package Java;

public class samp {

	public static void main(String[] args)
	{
String s1="bengaluru";
String s2="mysore";
         int length = s1.length()+s2.length();
         String[]arr=new String[length];
         
         int pos=0;
         for(String a:arr)
         {
        	 a=arr[pos];
        	 pos++;
         }
         

	}

}
